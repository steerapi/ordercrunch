div(data-role='header', data-position='fixed', data-tap-toggle='false', data-update-page-padding='false')
  a(href='#', data-slidemenu='#slidemenu', data-slideopen='false', data-icon='smico', data-corners='false', data-iconpos='notext') Menu
  h1 Welcome {{businessName}}
div(data-role='content')
  a(href="#pageTime", data-role='button', ng-click="")
    i.icon-time
    | Manage Hours
  a(href="#pageMenu",data-role='button', ng-click="")
    i.icon-th-list
    | Manage Menus
  a(href="#pageOrders",data-role='button', ng-click="")
    i.icon-shopping-cart
    | Manage Orders
  a(href="#pageEmployees",data-role='button', ng-click="")
    i.icon-user
    | Manage Employees
  a(href="#pageSettings",data-role='button', ng-click="")
    i.icon-cog
    | Manage Capacity and Settings
  a(href='#pageLogin',data-role='button', ng-click="logout()")
    i.icon-off
    | Logout
